<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Sci-GeoTracker</title>

    <meta name="viewport" content="initial-scale=1,maximum-scale=5,user-scalable=yes" />
    <script src="https://api.mapbox.com/mapbox-gl-js/v3.1.2/mapbox-gl.js"></script>
    <link href="https://api.mapbox.com/mapbox-gl-js/v3.1.2/mapbox-gl.css" rel="stylesheet" />
    <script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-draw/v1.4.3/mapbox-gl-draw.js"></script>
    <link
      rel="stylesheet"
      href="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-draw/v1.4.3/mapbox-gl-draw.css"
      type="text/css"
    />
    <script src="https://unpkg.com/@turf/turf@6/turf.min.js"></script>
    <script src="https://cdn.tailwindcss.com"></script>
    <script>
      tailwind.config = {
        darkMode: "class",
      };
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/7.8.5/d3.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.5/FileSaver.min.js"></script>
  </head>

  <body class="m-0 p-0 text-neutral-500 dark:text-neutral-300 fill-neutral-500 h-screen max-h-screen light">
    <div class="absolute top-0 left-0 z-20 m-2 flex flex-row gap-2 pointer-events-none">
      <div id="tray" class="flex flex-col gap-2 pointer-events-auto">
        <div
          class="flex flex-col overflow-hidden text-sm font-semibold rounded-lg ring-1 shadow-sm dark:ring-neutral-700/50 ring-neutral-300/50"
        >
          <!-- <button type="button" id="button" class="p-2 hover:bg-neutral-900">
            <svg class="h-5 w-5 rotate-45" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
              <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 3.75v4.5m0-4.5h4.5m-4.5 0L9 9M3.75 20.25v-4.5m0 4.5h4.5m-4.5 0L9 15M20.25 3.75h-4.5m4.5 0v4.5m0-4.5L15 9m5.25 11.25h-4.5m4.5 0v-4.5m0 4.5L15 15" />
            </svg>
          </button> -->
          <button
            type="button"
            alt="Drag and move map tool"
            id="move-button"
            class="p-1.5 dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 bg-neutral-100/90 hover:bg-neutral-100/60"
          >
            <img class="h-6 w-6" src="images/icons8-move-90.png" alt="Drag and move map tool" />
          </button>
          <button
            type="button"
            alt="Draw custom rectangle tool"
            id="rectangle-button"
            class="p-1.5 dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 bg-neutral-100/90 hover:bg-neutral-100/60"
          >
            <img class="h-6 w-6" src="images/icons8-rectangle-90.png" alt="Draw custom rectangle tool" />
          </button>
          <button
            type="button"
            alt="Draw custom polygon tool"
            id="polygon-button"
            class="p-1.5 dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 bg-neutral-100/90 hover:bg-neutral-100/60"
          >
            <img class="h-6 w-6" src="images/icons8-radar-plot-90.png" alt="Draw custom polygon tool" />
          </button>
        </div>
        <button
          type="button"
          alt="Filter map tool"
          id="filter-button"
          class="rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-100/60 ring-neutral-300/90"
        >
          <img class="h-6 w-6" src="images/icons8-filter-90.png" alt="Filter map tool" />
        </button>
        <button
          type="button"
          alt="Saved areas folder"
          id="folder-button"
          class="rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-100/60 ring-neutral-300/90"
        >
          <img class="h-6 w-6" src="images/icons8-folder-90.png" alt="Saved areas folder" />
        </button>
        <button
          type="button"
          alt="Display histogram"
          id="histogram-button"
          class="rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-100/60 ring-neutral-300/90"
        >
          <img class="h-6 w-6" src="images/icons8-futures-90.png" alt="Display histogram" />
        </button>
        <button
          type="button"
          alt="Change map theme"
          id="theme-button"
          class="rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-100/60 ring-neutral-300/90"
        >
          <img class="h-6 w-6" src="images/moon-4-64.png" alt="Change map theme" />
          <!-- Temp icon -->
        </button>
      </div>

      <div class="w-40 pointer-events-auto h-fit grow-0">
        <div>
          <button
            type="button"
            alt="Dropdown list for different filters"
            class="inline-flex w-full gap-x-1.5 px-3 py-1.5 text-sm font-semibold rounded-lg shadow-sm ring-1 dark:bg-neutral-900/90 dark:hover:bg-neutral-900 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-200/90 ring-neutral-300/50"
            id="layer-menu-button"
            aria-expanded="true"
            aria-haspopup="true"
          >
            <span id="layer-menu-button-text"></span>
            <img
              class="h-4 w-4 ml-auto my-auto"
              src="images/icons8-dropdown-stroke-1px-90.png"
              alt="Dropdown list for different filters"
            />
          </button>
        </div>

        <!--
          Dropdown menu, show/hide based on menu state.
      
          Entering: "transition ease-out duration-100"
            From: "transform opacity-0 scale-95"
            To: "transform opacity-100 scale-100"
          Leaving: "transition ease-in duration-75"
            From: "transform opacity-100 scale-100"
            To: "transform opacity-0 scale-95"
        -->
        <div
          id="layer-menu-items-container"
          class="z-20 mt-2 rounded-md overflow-hidden ring-1 focus:outline-none shadow-sm dark:bg-neutral-900/90 dark:ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50"
          role="menu"
          aria-orientation="vertical"
          aria-labelledby="menu-button"
          tabindex="-1"
          style="display: none"
        >
          <div class="#py-1" role="none">
            <!-- Active: "bg-neutral-900 text-neutral-200", Not Active: "text-neutral-100" -->
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="frames-item"
              >Frames</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="heatmap-item"
              >Heatmap</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="choropleth-item"
              >Choropleth</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="isarithmic-item"
              >Isarithmic</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="dot-density-item"
              >Dot Density</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="frame-overlaps-item"
              >Frame Overlaps</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="border-selection-item"
              >Border Selection</a
            >
          </div>
        </div>
      </div>
    </div>

    <div class="absolute top-0 right-0 z-10 m-2 flex flex-col gap-4 pointer-events-none">
      <div id="search-container" class="flex flex-row gap-2">
        <div class="pointer-events-auto flex flex-col gap-1 h-fit">
          <input
            type="text"
            name="price"
            id="search-bar"
            class="block #w-full rounded-xl py-1.5 px-3 #border-0 text-sm w-64 ring-1 shadow-sm focus:outline-none dark:bg-neutral-900/90 bg-neutral-100/90 dark:focus:bg-neutral-950/80 focus:bg-white dark:placeholder:text-neutral-400 placeholder:text-neutral-500 dark:ring-neutral-700/50 ring-neutral-300/90"
            placeholder="Search"
          />
          <div id="search-results-container" class="flex flex-col gap-1 overflow-y-auto max-h-96"></div>
        </div>
        <div class="flex flex-col gap-4">
          <button
            type="button"
            alt="Search by placename"
            id="search-button"
            class="pointer-events-auto max-h-fit rounded-lg p-2 shadow-sm ring-1 dark:bg-neutral-900/90 dark:hover:bg-neutral-800/70 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-100/60 ring-neutral-300/90"
          >
            <img class="h-4 w-4" src="images/icons8-search-90.png" alt="Search by placename" />
          </button>

          <div id="zoom-container" class="ml-auto">
            <div class="flex flex-col">
              <button
                type="button"
                alt="Increase zoom"
                id="zoom-in-button"
                class="pointer-events-auto z-20 rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 bg-neutral-100/90 dark:ring-neutral-700/50 ring-neutral-300/90 dark:hover:bg-neutral-800/70 hover:bg-neutral-200/90"
              >
                <img class="h-3 w-3" src="images/icons8-plus-90.png" alt="Increase zoom" />
              </button>
              <div id="zoom-scroll-bar" class="h-44 relative">
                <div class="absolute w-full h-full py-3">
                  <div
                    id="zoom-scroll-button"
                    class="pointer-events-auto cursor-grab active:cursor-grabbing relative w-full h-2 rounded-md -mt-1 ring-1 dark:bg-neutral-800 dark:ring-neutral-700 bg-neutral-100/90 ring-neutral-300/90"
                  ></div>
                </div>
                <div
                  class="pointer-events-auto w-1 h-full mx-auto ring-1 dark:bg-neutral-900/90 dark:ring-neutral-700/50 bg-neutral-200/90 ring-neutral-300/90"
                ></div>
              </div>
              <button
                type="button"
                alt="Decrease zoom"
                id="zoom-out-button"
                class="pointer-events-auto z-20 rounded-lg p-1.5 ring-1 shadow-sm dark:bg-neutral-900/90 bg-neutral-100/90 dark:ring-neutral-700/50 ring-neutral-300/90 dark:hover:bg-neutral-800/70 hover:bg-neutral-200/90"
              >
                <img class="h-3 w-3" src="images/icons8-minus-90.png" alt="Decrease zoom" />
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="absolute bottom-0 left-0 z-10 m-2 flex flex-col gap-4 pointer-events-none">
      <div class="w-40 pointer-events-auto h-fit grow-0">
        <!--
          Dropdown menu, show/hide based on menu state.
      
          Entering: "transition ease-out duration-100"
            From: "transform opacity-0 scale-95"
            To: "transform opacity-100 scale-100"
          Leaving: "transition ease-in duration-75"
            From: "transform opacity-100 scale-100"
            To: "transform opacity-0 scale-95"
        -->
        <div
          id="style-menu-items-container"
          class="z-20 mb-2 rounded-md overflow-hidden ring-1 focus:outline-none shadow-sm dark:bg-neutral-900/90 dark:ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50"
          role="menu"
          aria-orientation="vertical"
          aria-labelledby="menu-button"
          tabindex="-1"
          style="display: none"
        >
          <div class="#py-1" role="none">
            <!-- Active: "bg-neutral-900 text-neutral-200", Not Active: "text-neutral-100" -->
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="dark-item"
              >Dark</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="light-item"
              >Light</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="satellite-item"
              >Satellite</a
            >
            <a
              href="#"
              class="block px-4 py-2 text-sm dark:hover:bg-neutral-800/40 hover:bg-neutral-200/70"
              role="menuitem"
              tabindex="-1"
              id="topo-item"
              >Topology</a
            >
          </div>
        </div>
        <div>
          <button
            type="button"
            alt="Dropdown list for different filters"
            class="inline-flex w-full gap-x-1.5 px-3 py-1.5 text-sm font-semibold rounded-lg shadow-sm ring-1 dark:bg-neutral-900/90 dark:hover:bg-neutral-900 dark:ring-neutral-700/50 bg-neutral-100/90 hover:bg-neutral-200/90 ring-neutral-300/50"
            id="style-menu-button"
            aria-expanded="true"
            aria-haspopup="true"
          >
            <span id="style-menu-button-text"></span>
            <img
              class="h-4 w-4 ml-auto my-auto"
              src="images/icons8-dropdown-stroke-1px-90.png"
              alt="Dropdown list for different filters"
            />
          </button>
        </div>
      </div>
    </div>

    <div class="absolute bottom-0 right-0 z-10 m-2 flex flex-row gap-2">
      <div
        id="coords-container"
        class="py-1 px-6 #w-20 #h-10 rounded-sm dark:bg-neutral-900/90 ring-1 ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50 text-[11px] font-mono text-center dark:text-neutral-400 text-neutral-500"
      >
        <p id="coords" class="font-medium">-.000, -.000</p>
        <p id="scale" class="font-normal">10km</p>
      </div>
    </div>

    <div
      id="area-selection-info-container"
      class="absolute z-20 w-72 px-2 py-2 rounded-xl overflow-hidden text-sm dark:bg-neutral-900/90 ring-1 ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50 top-20 right-20"
      style="display: none"
    >
      <div class="flex flex-row gap-2 px-1">
        <h2 class="dark:text-neutral-300 text-neutral-500 grow font-bold">Area Selection</h2>
        <button type="button" id="area-selection-info-close-button" class="ml-auto">
          <img class="h-3 w-3" src="images/icons8-close-90.png" />
        </button>
      </div>
      <div class="my-2 px-1">
        <table class="table-auto border-separate border-spacing-y-1.5 w-full">
          <tbody>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Total area</td>
              <td id="selection-total-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="selection-total-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Covered area</td>
              <td id="selection-covered-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="selection-covered-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Uncovered area</td>
              <td id="selection-uncovered-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="selection-uncovered-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Coverage %</td>
              <td id="selection-coverage-percentage-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="selection-coverage-percentage-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                %
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Total missions</td>
              <td id="selection-total-missions-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <!-- <td
                id="selection-total-missions-unit"
                class="font-light text-right dark:text-neutral-200 text-neutral-500"
              ></td> -->
            </tr>
          </tbody>
        </table>
      </div>
      <div>
        <button
          type="button"
          id="area-selection-info-view-button"
          class="px-4 py-1 w-full rounded-md overflow-hidden dark:bg-neutral-800 ring-1 ring-neutral-600/50 ring-neutral-700/50 bg-neutral-300/90"
        >
          View
        </button>
      </div>
    </div>

    <div
      id="area-view-info-container"
      class="absolute z-20 w-72 px-2 py-2 rounded-xl overflow-hidden text-sm dark:bg-neutral-900/90 ring-1 ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50 top-20 right-20"
      style="display: none"
    >
      <div class="flex flex-row gap-2 px-1">
        <h2 class="dark:text-neutral-300 text-neutral-500 grow font-bold">Area View</h2>
        <button type="button" id="area-view-info-close-button" class="ml-auto">
          <img class="h-3 w-3" src="images/icons8-close-90.png" />
        </button>
      </div>
      <div class="my-2 px-1">
        <table class="table-auto border-separate border-spacing-y-1.5 w-full">
          <tbody>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Total area</td>
              <td id="view-total-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="view-total-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Covered area</td>
              <td id="view-covered-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="view-covered-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Uncovered area</td>
              <td id="view-uncovered-area-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="view-uncovered-area-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                km²
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Coverage %</td>
              <td id="view-coverage-percentage-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <td id="view-coverage-percentage-unit" class="font-light text-right dark:text-neutral-200 text-neutral-500">
                %
              </td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-200 text-neutral-500">Total missions</td>
              <td id="view-total-missions-value" class="font-light text-neutral-400 #font-medium #dark:text-neutral-200 #text-neutral-500">0</td>
              <!-- <td
                id="view-total-missions-unit"
                class="font-light text-right dark:text-neutral-200 text-neutral-500"
              ></td> -->
            </tr>
          </tbody>
        </table>
      </div>
      <div>
        <button
          type="button"
          id="area-view-info-view-button"
          class="px-4 py-1 w-full rounded-md overflow-hidden dark:bg-neutral-800 ring-1 ring-neutral-600/50 ring-neutral-700/50 bg-neutral-300/90"
        >
          View
        </button>
      </div>
    </div>

    <div
      id="flyto-mission-info-container"
      class="absolute z-20 w-100 px-4 py-2 rounded-xl overflow-hidden text-sm dark:bg-neutral-900/90 ring-1 ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50 top-20 start-20"
      style="display: none"
    >
      <div class="flex flex-row gap-2 px-1">
        <h2 class="dark:text-neutral-300 text-neutral-500 grow font-bold">Mission Data</h2>
        <button type="button" id="flyto-mission-info-close-button" class="ml-auto">
          <img class="h-3 w-3" src="images/icons8-close-90.png" />
        </button>
      </div>
      <div class="my-2 px-1">
        <table class="table-auto border-separate border-spacing-y-1.5 w-full">
          <tbody>
            <tr>
              <td class="font-light dark:text-neutral-400 text-neutral-500">Scene Name</td>
              <td
                id="flyto-mission-scene-name"
                class="font-medium text-right dark:text-neutral-200 text-neutral-500"
              ></td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-400 text-neutral-500">Mission Name</td>
              <td
                id="flyto-mission-mission-name"
                class="font-medium text-right dark:text-neutral-200 text-neutral-500"
              ></td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-400 text-neutral-500">Mission ID</td>
              <td
                id="flyto-mission-mission-id"
                class="font-medium text-right dark:text-neutral-200 text-neutral-500"
              ></td>
            </tr>
            <tr>
              <td class="font-light dark:text-neutral-400 text-neutral-500">Creator</td>
              <td id="flyto-mission-creator" class="font-medium text-right dark:text-neutral-200 text-neutral-500"></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div>
        <button
          type="button"
          id="flyto-mission-info-view-button"
          class="px-4 py-1 w-full rounded-md overflow-hidden dark:bg-neutral-800 ring-1 ring-neutral-600/50 ring-neutral-700/50 bg-neutral-300/90"
        >
          View
        </button>
      </div>
    </div>

    <div
      id="saved-areas-container"
      class="absolute z-30 w-96 h-fit px-2 py-2 rounded-xl overflow-hidden text-sm dark:bg-neutral-900/90 ring-1 ring-neutral-700/50 bg-neutral-100/90 ring-neutral-200/50 top-0 bottom-0 right-0 left-0 m-auto"
      style="display: none"
    >
      <div class="flex flex-row gap-2 px-1">
        <h2 class="dark:text-neutral-300 text-neutral-500 grow font-bold">Saved Areas</h2>
        <button type="button" id="saved-areas-close-button" class="ml-auto">
          <img class="h-3 w-3" src="images/icons8-close-90.png" />
        </button>
      </div>
      <div class="my-2 px-1 flex flex-col">
        <input
          type="text"
          name="saved-areas-search"
          id="saved-areas-search"
          class="block #w-full rounded-xl py-1.5 px-3 #border-0 text-sm my-2 #ring-inset shadow-sm dark:bg-neutral-600/90 bg-neutral-100/90 dark:focus:bg-neutral-650/80 focus:bg-white dark:placeholder:text-neutral-400 placeholder:text-neutral-500 dark:ring-neutral-700/50 ring-neutral-300/90"
          placeholder="Search"
        />
        <div id="saved-areas-list" class="h-52 overflow-y-scroll text-gray-500 dark:text-gray-400 flex flex-col gap-2">
        </div>
        <div class="flex flex-row gap-2 text-xs text-neutral-400 pt-2">
            
            <input type="file" name="file-upload" id='saved-areas-upload' multiple hidden>
            <button 
            type="button"
            id="saved-areas-import-button"
            class="px-4 py-1 w-full rounded-md overflow-hidden bg-neutral-800 ring-1 ring-neutral-600/50"
            onclick="open_file()">
              Import GEOJSON
            </button>
            <script type="text/javascript">
              function open_file() {
                document.getElementById('saved-areas-upload').click();
              }
            </script>
            <button
            type="button"
            id="saved-areas-export-button"
            class="px-4 py-1 w-full rounded-md overflow-hidden bg-neutral-800 ring-1 ring-neutral-600/50">
            Export GeoJSON
          </button>
        </div>
      </div>
    </div>
    

    <div id="timeline-container" class="absolute mx-auto my-2 w-fit bottom-0 left-0 right-0 z-10 bg-black opacity-70 shadow-lg rounded-lg">
    </div>

    <div id="map" class="absolute top-0 bottom-0 w-full z-0"></div>

    <style type="text/css">
      .timeline-axis {
        --daily-gridline-color: rgb(194, 199, 200);
        --yearly-gridline-color: rgb(81, 93, 93);
        --yearly-tick-color: rgb(81, 93, 93);
        --yearly-label-color: rgb(60, 79, 81);
        --daily-tick-color: rgb(161, 173, 173);
        --daily-label-color: rgb(60, 79, 81);
        --weekly-tick-color: rgb(161, 173, 173);
        --weekly-label-color: rgb(60, 79, 81);
      }

      .timeline-axis line {
        shape-rendering: geometricPrecision;
        stroke-width: 0.5;
      }

      .timeline-axis .yearly line {
        stroke: var(--yearly-tick-color);
      }

      .timeline-axis .yearly text {
        color: var(--yearly-label-color);
      }

      .timeline-axis .daily line {
        stroke: var(--daily-tick-color);
      }

      .timeline-axis .daily text {
        color: var(--daily-label-color);
      }

      .timeline-axis .weekly line {
        stroke: var(--weekly-tick-color);
      }

      .timeline-axis .weekly text {
        color: var(--weekly-label-color);
      }

      .timeline-axis .grid line {
        stroke: var(--daily-gridline-color);
      }

      .timeline-axis .yearlyGrid line {
        stroke: var(--yearly-gridline-color);
      }
    </style>
  </body>
  <script type="module" src="./javascripts/map.js"></script>
